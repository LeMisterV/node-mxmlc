{
  "name": "node-mxmlc",
  "description": "A Node.js module to download, \"install\" and use the latest open source Apache/Adobe Flex SDK.",
  "version": "1.0.1",
  "sdks": {
    "4.10.0": {
      "url": "http://archive.apache.org/dist/flex/4.10.0/binaries/apache-flex-sdk-4.10.0-bin.zip"
    },
    "4.13.0": {
      "url": "http://archive.apache.org/dist/flex/4.13.0/binaries/apache-flex-sdk-4.13.0-bin.tar.gz",
      "binpath": "apache-flex-sdk-4.13.0-bin/bin"
    },
    "4.14.1": {
      "url": "http://www.eu.apache.org/dist/flex/4.14.1/binaries/apache-flex-sdk-4.14.1-bin.tar.gz",
      "binpath": "apache-flex-sdk-4.14.1-bin/bin"
    },
    "4.15.0": {
      "url": "http://archive.apache.org/dist/flex/4.15.0/binaries/apache-flex-sdk-4.15.0-bin.tar.gz",
      "binpath": "apache-flex-sdk-4.15.0-bin/bin"
    }
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "main": "./node-mxmlc",
  "bin": {
    "mxmlc": "./bin/mxmlc"
  },
  "dependencies": {
